.zm-input{border:1px solid #f0f0f0;border-radius:4px}.image-list{display:flex;justify-content:center;box-sizing:border-box;padding:15px;padding-top:0;width:100%;background:#FFF;overflow:hidden}.image-list .choosed-image-list{position:relative;float:left;width:70px;height:70px;margin-right:15px;margin-top:15px}.image-list .choosed-image-list .choosed-image{width:100%;height:100%}.image-list .choosed-image-list .choosed-image-delete-btn{position:absolute;right:-10px;top:-10px;width:20px;height:20px}.image-list .choosed-image-list .choosed-image-delete-btn-icon{width:20px;height:20px}.image-list .choose-image-btn{float:left;width:70px;height:70px;margin-top:15px}.image-list .choose-image-btn .choose-image-btn-icon{width:70px;height:70px}.zm-invite{display:flex;flex-direction:column;justify-content:center;padding:50px 0}.zm-invite text{padding-bottom:10px;font-weight:700}.zm-invite button{margin-top:10px}.zm-edit-container{margin:10px;height:100%;box-shadow:0 2px 12px 0 rgba(0,0,0,0.1);border:1px solid #e6ebf5;border-radius:10px;box-shadow:inset 0 2px 12px 0 rgba(0,0,0,0.1)}.green{color:green}